                 <enumeratedValue>
                    <name>1</name>
                    <description>The polarity of off-chip USB VBUS power switch valid status is low</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
            </field>
          </fields>
