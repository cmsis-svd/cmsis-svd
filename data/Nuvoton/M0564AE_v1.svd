                 <enumeratedValue>
                    <name>1</name>
                    <description>Drive SC_DATA pin to high.\nSC_DATA signal status is high</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
