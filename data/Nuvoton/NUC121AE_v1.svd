                    <description>2 ADC clock for sampling ; 14 ADC clock for complete conversion</description>
                    <value>#000</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>3 ADC clock for sampling ; 15 ADC clock for complete conversion</description>
                    <value>#001</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>2</name>
                    <description>4 ADC clock for sampling ; 16 ADC clock for complete conversion</description>
                    <value>#010</value>
