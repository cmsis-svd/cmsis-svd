                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>Force CLKSTAT line high</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
            </field>
            <field>
