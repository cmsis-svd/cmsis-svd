                 <enumeratedValue>
                    <name>1</name>
                    <description>Indicates the PEC transmission/ receive is finished when the PECEN is set</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
            </field>
          </fields>
