                  <value>2</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>

        <register>
          <name>SLEEP</name>
          <description>Initiate sleep sequence.</description>
          <addressOffset>0x48</addressOffset>
        </register>

        <register>
          <name>KEY</name>
          <description>PMU key register.</description>
          <addressOffset>0x4C</addressOffset>
        </register>

      </registers>
    </peripheral> <!-- PMU -->

