            </field>
            <field>
              <name>DATSTS</name>
              <description>\n</description>
              <bitOffset>16</bitOffset>
              <bitWidth>1</bitWidth>
              <enumeratedValues>
                 <enumeratedValue>
                    <name>0</name>
                    <description>The SC_DAT pin is low</description>
