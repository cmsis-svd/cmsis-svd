                    <description>28 *TWDT</description>
                    <value>#010</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>3</name>
                    <description>210 *TWDT</description>
                    <value>#011</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>4</name>
                    <description>212 *TWDT</description>
                    <value>#100</value>
                 </enumeratedValue>
                 <enumeratedValue>
