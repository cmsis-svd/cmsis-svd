    <peripheral>
      <name>I2S1</name>
      <alternatePeripheral>FLEXCOMM7</alternatePeripheral>
      <description>LPC5411x I2S interface</description>
      <groupName>I2S</groupName>
      <baseAddress>0x40098000</baseAddress>
