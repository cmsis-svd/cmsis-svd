                 <enumeratedValue>
                    <name>1</name>
                    <description>Chip entering the Power-down mode instantly or wait CPU Idle command</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
