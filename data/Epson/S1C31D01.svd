    <!-- DMAC - PrimeCell uDMA Controller -->
    <peripheral>
      <name>DMAC</name>
      <version>1.0</version>
      <description>PrimeCell uDMA Controller</description>
      <baseAddress>0x40001000</baseAddress>
      <size>32</size>
      <access>read-write</access>
      <addressBlock>
        <offset>0</offset>
        <size>0x1018</size>
        <usage>registers</usage>
      </addressBlock>
      <interrupt>
        <name>DMAC</name>
        <!-- DMA Transfer complete, Transfer error-->
        <value>0</value>
      </interrupt>
      <registers>
        <!-- STAT: DMAC Status Register -->
        <register>
          <name>STAT</name>
          <description>Returns the DMAC status </description>
          <addressOffset>0x00</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x10030000</resetValue>
          <fields>
            <!-- CHNLS -->
            <field>
              <name>CHNLS</name>
              <description>Number of available DMAC channels minus one</description>
              <bitRange>[20:16]</bitRange>
              <access>read-only</access>
            </field>
            <!-- STATE[3:0] -->
            <field>
              <name>STATE</name>
              <description>These bits indicates the DMA transfer status</description>
              <bitRange>[7:4]</bitRange>
              <access>read-only</access>
            </field>
            <!-- MSTENSTAT: -->
            <field>
              <name>MSTENSTAT</name>
              <description>This bit indicates the DMA controller status</description>
              <bitRange>[0:0]</bitRange>
              <access>read-only</access>
            </field>
          </fields>
        </register>
        <!-- CFG: DMAC configuration -->
        <register>
          <name>CFG</name>
          <description>DMA configuration</description>
          <addressOffset>0x04</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <fields>
            <!-- MSTEN:  -->
            <field>
              <name>MSTEN</name>
              <description>This bit enables the DMA controller.</description>
              <bitRange>[0:0]</bitRange>
              <access>write-only</access>
            </field>
          </fields>
        </register>
        <!-- CPTR[31:0]: DMAC Pointer to the base address of the primary data structure -->
        <register>
          <name>CPTR</name>
          <description>Pointer to the base address of the primary data structure</description>
          <addressOffset>0x08</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <fields>
            <!-- CPTR -->
            <field>
              <name>CPTR</name>
              <description>DMAC Control Data Base Pointer Reg</description>
              <bitRange>[31:0]</bitRange>
              <access>read-write</access>
            </field>
          </fields>
        </register>
        <!-- ACPTR[31:0]: DMAC Pointer to the base address of the alternative data structure -->
        <register>
          <name>ACPTR</name>
          <description>Pointer to the base address of the alternative data structure</description>
          <addressOffset>0x0C</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <fields>
            <field>
              <name>ACPTR</name>
              <description>These bits show the alternate data structure base address</description>
              <access>read-only</access>
              <bitRange>[31:0]</bitRange>
            </field>
          </fields>
        </register>
        <!-- SWREQ[31:0]: DMAC Software Request Register -->
        <register>
          <name>SWREQ</name>
          <description>The write-only register enables you to generate a software DMA request</description>
          <addressOffset>0x14</addressOffset>
          <size>32</size>
          <access>write-only</access>
          <fields>
            <field>
              <name>SWREQ</name>
              <description>These bits issue a software DMA transfer request to each channel</description>
              <access>write-only</access>
              <bitRange>[3:0]</bitRange>
            </field>
          </fields>
        </register>
        <!-- RMSET[31:0]: DMAC Request Mask Set Register -->
        <register>
          <name>RMSET</name>
          <description>Setting this bit to 1 causes the DMA channel to no longer respond to data requests from peripherals. The channel will always respond to software transfer requests</description>
          <addressOffset>0x20</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <fields>
            <field>
              <name>RMSET</name>
              <description>Setting this bit to 1 causes the DMA channel to no longer respond to data requests from peripherals. The channel will always respond to software transfer requests</description>
              <access>read-write</access>
              <bitRange>[3:0]</bitRange>
            </field>
          </fields>
        </register>
        <!-- RMCLR[31:0]:DMAC Request Mask Clear Register  -->
        <register>
          <name>RMCLR</name>
          <description>Firmware can write a 1 to the register to clear the mask for a channel</description>
          <addressOffset>0x24</addressOffset>
          <size>32</size>
          <access>write-only</access>
          <fields>
            <field>
              <name>RMCLR</name>
              <description>Firmware can write a 1 to the register to clear the mask for a channel</description>
              <access>write-only</access>
              <bitRange>[3:0]</bitRange>
            </field>
          </fields>
        </register>
