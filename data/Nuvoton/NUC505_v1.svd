                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
            </field>
          </fields>
       </register>
       <register>
          <name>HCRHPORTSTATUS1</name>
          <!-- the display name is an unrestricted string. -->
          <displayName>HCRHPORTSTATUS1</displayName>
