        <register>
          <name>PASET</name>
          <description>These bits enable the alternate data structures. Returns status of alternate structures.</description>
          <addressOffset>0x30</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <fields>
            <field>
              <name>PASET</name>
              <description>These bits enable the alternate data structures. Returns status of alternate structures.
</description>
              <access>read-write</access>
              <bitRange>[3:0]</bitRange>
            </field>
          </fields>
        </register>
        <!-- PACLR[31:0]: DMAC Primary-Alternate Clear Register -->
        <register>
          <name>PACLR</name>
          <description> These bits disable the alternate data structures</description>
          <addressOffset>0x34</addressOffset>
          <size>32</size>
          <access>write-only</access>
          <fields>
