              <enumeratedValues>
                 <enumeratedValue>
                    <name>0</name>
                    <description>Time stamp counter is in coarse update mode</description>
                    <value>#0</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>Time stamp counter is in fine update mode</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
            </field>
            <field>
              <name>TSUPDATE</name>
