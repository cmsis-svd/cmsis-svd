      <name>RTCTMR</name>
      <description>Real Time Clock</description>
      <groupName>Real-Time Clock</groupName>
      <baseAddress>0x40000A00</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <addressBlock>
        <offset>0</offset>
        <size>0x0030</size>
        <usage>registers</usage>
      </addressBlock>
      <interrupt>
        <name>RTC_COMP0</name>
