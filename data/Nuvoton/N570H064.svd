            <field>
              <name>RTS</name>
              <description>RTS (Request-to-send) Signal \n</description>
              <bitOffset>1</bitOffset>
              <bitWidth>1</bitWidth>
              <enumeratedValues>
                 <enumeratedValue>
                    <name>0</name>
                    <description>Drive RTS inactive ( = ~RTSACTLV)</description>
                    <value>#0</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>Drive RTS active ( = RTSACTLV)</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
