                    <name>0</name>
                    <description>Slave time-out does not occur yet</description>
                    <value>#0</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>Slave time-out has occurred</description>
                    <value>#1</value>
