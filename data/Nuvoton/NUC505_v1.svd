            <field>
              <name>DBR16</name>
              <description>Data Buffer Region 16\nWhen set, the size of the data buffer region is 16 bytes. Otherwise, the size is 32 bytes.</description>
              <bitOffset>0</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
            </field>
            <field>
              <name>ABORT</name>
