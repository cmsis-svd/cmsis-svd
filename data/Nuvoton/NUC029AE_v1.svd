              </enumeratedValues>
              <access>read-only</access>
            </field>
            <field>
              <name>RLS_INT</name>
              <description>Receive Line Status Interrupt  (Read Only)\nThis bit is set if RLS_IEN and RLS_IF are both set to 1.</description>
              <bitOffset>10</bitOffset>
