<device schemaVersion="1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="CMSIS-SVD_Schema_1_1.xsd" >
  <vendor>NXP</vendor>
  <name>JN5179</name>
  <series>JN517x</series>
  <version>20150316</version>
  <description>NXP JN5179 Wireless MCU: Cortex-M3 Microcontroller with 512KB Flash, 32KB RAM, IEEE802.15.4 Radio (refer to http://www.nxp.com/techzones/wireless-connectivity/overview.html for more)</description>
  <cpu>
    <name>CM3</name>
    <revision>r2p1</revision>
    <endian>little</endian>
    <mpuPresent>false</mpuPresent>
    <fpuPresent>false</fpuPresent>
    <nvicPrioBits>4</nvicPrioBits>
    <vendorSystickConfig>false</vendorSystickConfig>
  </cpu>
  <addressUnitBits>8</addressUnitBits>
  <width>32</width>
  
  <!-- Defaults for peripheral registers -->
  <size>32</size>
  <access>read-write</access>
  <resetValue>0x00000000</resetValue>
  <resetMask>0xFFFFFFFF</resetMask>
  
  <peripherals>
  
    <peripheral>
      <name>SYS_CTRL</name>
      <version>1.0</version>
      <description>System Controller</description>
      <groupName>SYSTEM_CONTROLLER</groupName>
      <baseAddress>0x40000000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <addressBlock>
        <offset>0</offset>
        <size>0x200</size>
        <usage>registers</usage>
      </addressBlock>

      <registers>
      <!-- PWR_CTRL: Power Control -->
        <register>
          <name>PWR_CTRL</name>
          <description>Power Control Register</description>
          <addressOffset>0x00</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <fields>
            <field>
              <name>PDR</name>
              <description>Power Down Request (once set the chip begins power down sequence and it must not be cleared)</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Power down not requested</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Power down requested</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>DSS</name>
              <description>Deep sleep request</description>
              <bitRange>[1:1]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Sleep</name>
                  <description>Select normal sleep mode</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>DeepSleep</name>
                  <description>Select deep sleep mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PCEN</name>
              <description>Protocol Clock Enable</description>
              <bitRange>[3:3]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Protocol clock disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Protocol clock enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>SPIMEN</name>
              <description>SPI Master Enable</description>
              <bitRange>[4:4]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>SPI master disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>SPI master enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>MHUP</name>
              <description>Memory Hold Up</description>
              <bitRange>[5:5]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>RAM is powered down during sleep</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>RAM remains powered during sleep</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PWM6EN</name>
              <description>Enable PWM6 Timer</description>
              <bitRange>[6:6]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>PWM6 timer is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>PWM6 timer is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PWM5EN</name>
              <description>Enable PWM5 Timer</description>
              <bitRange>[7:7]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>PWM5 timer is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>PWM5 timer is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>TADCEN</name>
              <description>Enable ADC Timer</description>
              <bitRange>[8:8]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>ADC timer is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>ADC timer is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>T1EN</name>
              <description>Enable Timer 1</description>
              <bitRange>[9:9]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Timer 1 is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Timer 1 is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>UEN0</name>
              <description>Enable UART 0</description>
              <bitRange>[12:12]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>UART 0 is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>UART 0 is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>UEN1</name>
              <description>Enable UART 1</description>
              <bitRange>[13:13]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>UART 1 is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>UART 1 is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>T0EN</name>
              <description>Enable Timer 0</description>
              <bitRange>[14:14]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Timer 0 is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Timer 0 is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>SPISEN</name>
              <description>Enable SPI Slave</description>
              <bitRange>[15:15]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>SPI slave is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>SPI slave is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>I2CEN</name>
              <description>Enable I2C Interface</description>
              <bitRange>[16:16]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>I2C interface is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>I2C interface is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>NASSEN</name>
              <description>Number of additional SPI slave selects</description>
              <bitRange>[18:17]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>SPI slave select available for DIO6 (depending on DIO configuration)</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>SPI slave select available for DIO6, DIO15 (depending on DIO configuration)</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>2</name>
                  <description>SPI slave select available for DIO6, DIO7 (depending on DIO configuration)</description>
                  <value>2</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PWM4EN</name>
              <description>Enable PWM4 Timer</description>
              <bitRange>[20:20]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>PWM4 Timer is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>PWM4 Timer is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PD32K</name>
              <description>Power Down 32kHz Oscillator during sleep</description>
              <bitRange>[21:21]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>32kHz Oscillator remains powered during sleep</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>32kHz Oscillator is powered down during sleep</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>APEN</name>
              <description>Enable Analogue Peripheral Power</description>
              <bitRange>[22:22]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Analogue periperal power is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Analogue periperal power is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PWM1EN</name>
              <description>Enable PWM1 Timer</description>
              <bitRange>[28:28]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>PWM1 Timer is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>PWM1 Timer is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PWM2EN</name>
              <description>Enable PWM2 Timer</description>
              <bitRange>[29:29]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>PWM2 Timer is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>PWM2 Timer is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PWM3EN</name>
              <description>Enable PWM3 Timer</description>
              <bitRange>[30:30]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>PWM3 Timer is disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>PWM3 Timer is enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        
        <!-- STAT: Status Information -->
        <register>
          <name>STAT</name>
          <description>Device Status Register</description>
          <addressOffset>0x04</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <fields>
            <field>
              <name>WUS</name>
              <description>Wake Up Status</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>ColdStart</name>
                  <description>Device cold start</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>WarmStart</name>
                  <description>Device warm start</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>MS</name>
              <description>Memory Status</description>
              <bitRange>[1:1]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>MemoryClear</name>
                  <description>Memory was powered down during sleep</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>MemoryHeld</name>
                  <description>Memory was held during sleep</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>APPS</name>
              <description>Analogue Periperhal Power Status</description>
              <bitRange>[2:2]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Analogue peripherals are powered up</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Analogue peripherals are powered off</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PCS</name>
              <description>IEEE802.15.4 Radio Clock Status</description>
              <bitRange>[3:3]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Protocol clock is stopped</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Protocol clock is running</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>MISOS</name>
              <description>Value of SPIMISO pin</description>
              <bitRange>[4:4]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>SPIMISO pin is low</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>SPIMISO pin is high</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>VBO_STAT</name>
              <description>Voltage Brown Out Status</description>
              <bitRange>[5:5]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Supply voltage is greater than VBO threshold</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Supply voltage is lower than VBO threshold</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>VBO_STAT_VAL</name>
              <description>Voltage Brown Out Status Valid</description>
              <bitRange>[6:6]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Voltage brown out status is invalid</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Voltage brown out status is valid</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>WD_RES</name>
              <description>Watchdog Reset Event</description>
              <bitRange>[7:7]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Normal power up</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Power up was proceeded by a watchdog timeout</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>VBO_RES</name>
              <description>Voltage Brown Out Reset Event</description>
              <bitRange>[8:8]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Normal power up</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Power up was proceeded by a voltage brown out</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>CLK32K</name>
              <description>Instantaneous value of the 32kHz system clock</description>
              <bitRange>[9:9]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>Clock is low</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>Clock is high</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>CLK32K_RDY</name>
              <description>Status of the 32kHz system clock</description>
              <bitRange>[10:10]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>Clock is not running</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>Clock is running satisfactorily</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>DS_WAKEUP</name>
              <description>Deep sleep wake up</description>
              <bitRange>[11:11]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Normal power up</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Power up was from deep sleep mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        
        <!-- SYS_IM_1: System Interrrupt Mask 1 -->
        <register>
          <name>SYS_IM_1</name>
          <description>System Interrrupt Mask 1 - Enable system controller interrupt sources</description>
          <addressOffset>0x0C</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <fields>
            <field>
              <name>PC0EM</name>
              <description>Pulse Counter 0 Event Mask</description>
              <bitRange>[22:22]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Pulse Counter 0 Interrupt Disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Pulse Counter 0 Interrupt Enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>PC1EM</name>
              <description>Pulse Counter 1 Event Mask</description>
              <bitRange>[23:23]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>Pulse Counter 1 Interrupt Disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>Pulse Counter 1 Interrupt Enabled</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            
            <field>
              <name>VFEM</name>
              <description>VBO Falling Event Mask (Voltage moved above threshold)</description>
              <bitRange>[24:24]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disabled</name>
                  <description>VBO Falling Interrupt Disabled</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enabled</name>
                  <description>VBO Falling Interrupt Enabled</description>
                  <value>1</value>
