                  <description>1 LSB =  300 nA</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>IDAC_HI</name>
                  <description>1 LSB = 2400 nA</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>IDAC_MED2</name>
                  <description>1 LSB = 1200 nA</description>
                  <value>3</value>
                </enumeratedValue>
