      <interrupt>
        <name>RTC</name>
        <!-- Peripheral bus fault interrupt-->
        <value>4</value>
      </interrupt>
      <registers>
        <!--CTL register is divided to Byte boundry.
If CTL register is 16bits register, theoritical regulation is accidentally started by accessing RTCTRM bit field when low byte bit field is accessed using Read-Modify-Write.-->
        <register>
          <name>CTLH</name>
          <description>Control Register high byte</description>
          <addressOffset>0x01</addressOffset>
          <size>8</size>
          <access>read-write</access>
          <resetValue>0x00</resetValue>
          <fields>
            <field>
              <name>RTCTRMBSY</name>
              <description>Trimming busy flag</description>
              <bitRange>[7:7]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>IDLE</name>
                  <description>Theoretical regulation has finished (or not executed)</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>BUSY</name>
                  <description>Theoretical regulation is executing</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>RTCTRM</name>
              <description>Theoretical regulation trimming value (2's complement value in 1/256 second)</description>
              <bitRange>[6:0]</bitRange>
              <access>write-only</access>
            </field>
          </fields>
        </register>
        <register>
          <name>CTLL</name>
          <description>Control Register low byte</description>
          <addressOffset>0x00</addressOffset>
          <size>8</size>
          <access>read-write</access>
          <resetValue>0x00</resetValue>
          <fields>
            <field>
              <name>RTCBSY</name>
              <description>Counter Busy flag</description>
              <bitRange>[6:6]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>IDLE</name>
                  <description>Idle (ready to rewrite real-time clock counter)</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>BUSY</name>
                  <description>In count-up operation</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>RTCHLD</name>
              <description>Counter Hold control</description>
              <bitRange>[5:5]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>NORMAL</name>
                  <description>Normal operation</description>
