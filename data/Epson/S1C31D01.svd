    <!-- Real-Time Clock
CTL register is divided to byte boundery (CTLH and CTLL).
When -->
    <peripheral>
      <name>RTCA</name>
      <version>1.0</version>
      <description>Real-time clock control</description>
      <baseAddress>0x400000C0</baseAddress>
