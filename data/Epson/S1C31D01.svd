          <addressOffset>0x2C</addressOffset>
          <size>32</size>
          <access>write-only</access>
          <fields>
            <field>
              <name>ENCLR</name>
              <description> Set the appropriate bit to disable the corresponding DMA channel</description>
              <access>write-only</access>
              <bitRange>[3:0]</bitRange>
            </field>
          </fields>
        </register>
