    <peripheral>
      <name>UART3_0</name>
      <version>1.0</version>
      <description>UART3 channel 0 control</description>
      <baseAddress>0x40000380</baseAddress>
      <size>16</size>
      <access>read-write</access>
      <addressBlock>
        <offset>0</offset>
        <size>0x16</size>
        <usage>registers</usage>
      </addressBlock>
      <interrupt>
        <name>UART3_0</name>
        <!-- UART EOT, Framing error, Parity error, Overrun error, Receive buffer 2 bytes full, Receive buffer 1 byte full, Transmit buffer empty interrupt -->
        <value>6</value>
      </interrupt>
      <registers>
        <!-- CLK: Clock Control Register -->
        <register>
          <name>CLK</name>
          <description>Clock Control Register</description>
          <addressOffset>0x00</addressOffset>
          <size>16</size>
          <access>read-write</access>
          <resetValue>0x0000</resetValue>
          <fields>
            <!-- DBRUN -->
            <field>
              <name>DBRUN</name>
              <description>Enable clock on debug mode</description>
              <bitRange>[8:8]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>DISABLE</name>
                  <description>Disable</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ENABLE</name>
                  <description>Enable</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- CLKDIV[1:0] -->
            <field>
              <name>CLKDIV</name>
              <description>Clock division select</description>
              <bitRange>[5:4]</bitRange>
              <access>read-write</access>
            </field>
            <!-- CLKSRC[1:0] -->
            <field>
              <name>CLKSRC</name>
              <description>Clock source select (can be write protected)</description>
              <bitRange>[1:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>IOSC</name>
                  <description>IOSC</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>OSC1</name>
                  <description>OSC1</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>OSC3</name>
                  <description>OSC3</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>EXOSC</name>
                  <description>EXOSC</description>
                  <value>3</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        <!-- MOD: Mode Register -->
        <register>
          <name>MOD</name>
          <description>Mode Register</description>
          <addressOffset>0x02</addressOffset>
          <size>16</size>
          <access>read-write</access>
          <resetValue>0x0000</resetValue>
          <fields>
            <!--PECAR-->
            <field>
