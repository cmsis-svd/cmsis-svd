                    <name>0</name>
                    <description>PS2TXDATA0[7:0]</description>
                    <value>#0000</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>PS2TXDATA0[15:8]</description>
                    <value>#0001</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>2</name>
                    <description>PS2TXDATA0[23:16]</description>
                    <value>#0010</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>3</name>
                    <description>PS2TXDATA0[31:24]</description>
                    <value>#0011</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>4</name>
                    <description>PS2TXDATA1[7:0]</description>
                    <value>#0100</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>5</name>
                    <description>PS2TXDATA1[15:8]</description>
                    <value>#0101</value>
                 </enumeratedValue>
                 <enumeratedValue>
