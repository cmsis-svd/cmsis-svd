              <enumeratedValues>
                <enumeratedValue>
                  <name>_1_BIT</name>
                  <description>1 stop bit.</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>_2_BITS</name>
