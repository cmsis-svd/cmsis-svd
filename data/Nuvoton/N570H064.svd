                 <enumeratedValue>
                    <name>0</name>
                    <description>Time-out frequency is 0.25Hz,</description>
                    <value>#000</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>Time-out frequency is 0.5Hz,</description>
                    <value>#001</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>2</name>
                    <description>Time-out frequency is 1Hz,</description>
                    <value>#010</value>
